<Page x:Class="Imp.Dash.MainPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="using:Imp.Dash"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      mc:Ignorable="d">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <StackPanel>
            <TextBlock FontFamily="ms-appx:///Imp.Dash.Cook/Fonts/ARCADE_I.TTF#Arcade Interlaced"
                       FontSize="40"
                       Text="Banana" />
            <TextBlock FontFamily="ms-appx:///Imp.Dash.Cook/Fonts/ARCADE_N.TTF#Arcade Normal"
                       FontSize="40"
                       Text="Banana" />
            <TextBlock FontFamily="ms-appx:///Imp.Dash.Cook/Fonts/ARCADE_R.TTF#Arcade Rounded"
                       FontSize="40"
                       Text="Banana" />
        </StackPanel>
    </Grid>
</Page>